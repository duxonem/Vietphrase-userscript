{
"name":"VietPhrase enxtension",
"description":"Convert vietphrase the chinese webpage to Vietnamese on the fly with strucphrase ability",
"version":"0.0.0.1",
"manifest_version":2,

"permissions": [
  "storage"
],

"background": {
  "scripts": ["background.js"]
},

"options_ui": {
  "page": "options.html",
  "open_in_tab":true
},

"browser_action": {
  "default_popup":"popup.html",
  "default_icon": "icon.png"
},

"content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }
]

}